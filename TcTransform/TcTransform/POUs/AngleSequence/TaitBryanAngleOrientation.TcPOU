    <Method Name="SetAngle" Id="{4ebf317e-4620-0dbb-0fd2-2805540c84d9}" FolderPath="Setters\">
      <Declaration><![CDATA[METHOD PUBLIC SetAngle : BOOL
VAR_INPUT
	I : UINT;
	Val : LREAL;
END_VAR
VAR
	Dim : Dimension;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF I >= Length THEN
	SetAngle := FALSE;
	RETURN;
END_IF

