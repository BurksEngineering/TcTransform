    <Method Name="GetAngle" Id="{f6726e47-c5fb-0990-0956-4dd56856561f}" FolderPath="Getters\">
      <Declaration><![CDATA[METHOD PUBLIC  GetAngle : LREAL
VAR_INPUT
	I : UINT;
END_VAR
VAR
	Dim : Dimension;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF I >= Length THEN
	GetAngle := 0;
	RETURN;
END_IF

Dim := GetDimension(I);

IF Dim >= Length THEN
	GetAngle := 0;
	RETURN;
END_IF

GetAngle := Angles_[Dim];
]]></ST>
      </Implementation>
    </Method>
    <Method Name="GetDimension" Id="{69e98eb8-908f-0785-2692-8746de432a97}" FolderPath="Getters\">
      <Declaration><![CDATA[METHOD PUBLIC GetDimension : Dimension
VAR_INPUT
	I : UINT;
END_VAR
VAR
	Dims : ARRAY [0..2] OF Dimension;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF I >= Length THEN
	GetDimension := 0;
	RETURN;
END_IF

Dims := GetDimensions();
GetDimension := Dims[I];]]></ST>
      </Implementation>
    </Method>
    <Method Name="GetDimensions" Id="{2cd7e7e1-f488-048d-091d-9e023d75faaa}" FolderPath="Getters\">
      <Declaration><![CDATA[METHOD PUBLIC GetDimensions : ARRAY [0..2] OF Dimension
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE Sequence OF
	TaitBryanAngleSequence.XYZ:
		GetDimensions[0] := 0;
		GetDimensions[1] := 1;
		GetDimensions[2] := 2;
	TaitBryanAngleSequence.XZY:
		GetDimensions[0] := 0;
		GetDimensions[1] := 2;
		GetDimensions[2] := 1;
	TaitBryanAngleSequence.YXZ:
		GetDimensions[0] := 1;
		GetDimensions[1] := 0;
		GetDimensions[2] := 2;
	TaitBryanAngleSequence.YZX:
		GetDimensions[0] := 1;
		GetDimensions[1] := 2;
		GetDimensions[2] := 0;
	TaitBryanAngleSequence.ZXY:
		GetDimensions[0] := 2;
		GetDimensions[1] := 0;
		GetDimensions[2] := 1;
	TaitBryanAngleSequence.ZYX:
		GetDimensions[0] := 2;
		GetDimensions[1] := 1;
		GetDimensions[2] := 0;
	ELSE
		GetDimensions[0] := 0;
		GetDimensions[1] := 0;
		GetDimensions[2] := 0;
END_CASE]]></ST>
      </Implementation>
    </Method>
